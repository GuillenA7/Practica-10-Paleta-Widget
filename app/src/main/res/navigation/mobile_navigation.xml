<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.prctica10_paletawidget.ui.galeria.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_galeria" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.prctica10_paletawidget.ui.credito.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_nosotros" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.prctica10_paletawidget.ui.nosotros.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_credito" />
